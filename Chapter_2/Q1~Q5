#include <iostream>
using namespace std;

int main() {
 for(int i=0; i<10;i++){
    for(int j=1; j<11; j++){
      cout<<j+i*10<<"\t";
    }
    cout<<endl;
  }
  return 0;
}
___________________________________________
#include <iostream>
using namespace std;

int main() {
  for(int i=1;i<10;i++){
    for(int j=1; j<10; j++){
      cout<<j<<"x"<<i<<"="<<j*i<<"\t";
    }
    cout<<endl;
  }
  return 0;
}
__________________________________________
#include <iostream>
using namespace std;

int main() {
  int a,b;
  cout<<"두 수를 입력하라>>";
  cin>>a>>b;
  if(a>b)
    cout<<"큰 수 = "<<a;
  else
    cout<<"큰 수 = "<<b;
  return 0;
}
____________________________________________
#include <iostream>
using namespace std;

int main() {
  double a,b;
  cout<<"5 개의 실수를 입력하라>>";
  
  cin>>a;
  for(int i=0;i<4;i++){
    cin>>b;
    if(b>a)
      a=b;
  }
  cout<<"제일 큰 수 = "<<a;
  return 0;
}
______________________________________
#include <string.h>
#include <string>
#include <iostream>
using namespace std;

int main() {
  cout<<"문자들을 입력하라(100개 미만)."<<endl;
  char str[100];
  int count=0;
  cin.getline(str,100);
  for(int i=0;i<strlen(str);i++){
    if(str[i]=='x')
      count++;
  }
  cout<<"x의 개수는 "<<count;
  
  return 0;
}
